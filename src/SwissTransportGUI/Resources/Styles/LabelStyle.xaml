<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="LargeText" TargetType="Label">
        <Setter Property="Foreground" Value="{StaticResource NormalTextColor}" />
        <Setter Property="FontWeight" Value="DemiBold" />
        <Setter Property="FontSize" Value="12" />
    </Style>
    <Style x:Key="LightText" TargetType="Label">
        <Setter Property="Foreground" Value="{StaticResource LightTextColor}" />
        <Setter Property="FontWeight" Value="DemiBold" />
        <Setter Property="FontSize" Value="12" />
    </Style>
</ResourceDictionary>